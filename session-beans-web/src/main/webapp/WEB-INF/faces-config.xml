<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>singleton-index</from-outcome>
            <to-view-id>/views/singleton/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>singleton-two</from-outcome>
            <to-view-id>/views/singleton/two-references.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>stateful-index</from-outcome>
            <to-view-id>/views/stateful-sb/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>stateful-two</from-outcome>
            <to-view-id>/views/stateful-sb/two-references.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>stateless-index</from-outcome>
            <to-view-id>/views/stateless-sb/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>stateless-two</from-outcome>
            <to-view-id>/views/stateless-sb/two-references.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
         <navigation-case>
            <from-outcome>concurrency-management</from-outcome>
            <to-view-id>/views/singleton/concurrency-management.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
         <navigation-case>
            <from-outcome>multiple-interfaces</from-outcome>
            <to-view-id>/views/multiple-interfaces/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
         <navigation-case>
            <from-outcome>async-task</from-outcome>
            <to-view-id>/views/async/async-task.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>
